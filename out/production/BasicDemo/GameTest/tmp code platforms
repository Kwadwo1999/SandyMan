      protected void paintBackground(Graphics2D g){ g.drawImage(background,0,0, (ImageObserver) this);
    }

    protected void paintForeground(Graphics2D g) {
        // System.out.println("called");
        g.drawString("red ball score: "+getGoodCharacter().getHealthCount()+ "   health: "+getGoodCharacter().getHealthCount(),60,100);

    }




protected void paintBackground(Graphics2D g){
        g.drawImage(background,0,0, (ImageObserver) this);
    }

    @Override
    protected void paintForeground(Graphics2D g) {
        // System.out.println("called");
        g.drawString("red ball score: "+getCowboy().getRedBallCount()+ "   health: "+getCowboy().getHealthCount(),60,100);

    }